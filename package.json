{
  "name": "assure-health-api",
  "version": "1.0.0",
  "description": "A basic API to interract with the client side",
  "main": "dist/server.js",
  "scripts": {
    "clean": "rm -rf dist build",
    "build": "npm run clean && tsc && tsc-alias",
    "start": "npm run build && pm2 start ecosystem.config.js --only assure-health-api-prod",
    "dev": "npm run build && pm2 start ecosystem.config.js --only assure-health-api-dev",
    "dev:watch": "cross-env NODE_ENV=development nodemon --watch 'src/**/*.ts' --exec 'ts-node -r tsconfig-paths/register' src/server.ts",
    "test": "cross-env NODE_ENV=test npm run migration && nyc --reporter=html --reporter=text mocha -r ts-node/register -r tsconfig-paths/register './src/tests/**/*.test.ts' --timeout 10000 --recursive --exit",
    "test:watch": "cross-env NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register './src/tests/**/*.test.ts' --timeout 10000 --recursive --watch",
    "coverage": "nyc npm run test && nyc report --reporter=text-lcov --reporter=lcov",
    "lint": "eslint src/**/*.ts --fix",
    "lint:check": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "migration": "npm run migrate:undo && npm run migrate",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:create": "npx sequelize-cli migration:generate --name",
    "seeder": "npm run seed:undo && npm run seed",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "seed:create": "npx sequelize-cli seed:generate --name",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Extremophile-ai/Assure-health-api.git"
  },
  "author": "Godspower Uche",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Extremophile-ai/Assure-health-api/issues"
  },
  "homepage": "https://github.com/Extremophile-ai/Assure-health-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/node": "^7.12.6",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/register": "^7.12.1",
    "@babel/runtime": "^7.12.5",
    "@types/bcrypt": "^6.0.0",
    "@types/chai": "^5.2.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.5.2",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^11.7.2",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^3.1.10",
    "nyc": "^15.1.0",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@sendgrid/mail": "^8.1.6",
    "bcrypt": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.3.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "pm2": "^6.0.13",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-cli": "^6.6.3",
    "sequelize-typescript": "^2.1.6"
  }
}
