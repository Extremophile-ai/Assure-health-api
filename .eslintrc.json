{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "consistent-return":"off",
        "import/no-dynamic-require":"off",
        "no-unused-vars":"off",
        "prefer-const": "off",
        "comma-dangle":"off",
        "no-undef":"off"
    }
}
