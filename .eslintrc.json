{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint"],
    "rules": {
        "no-console": "off",
        "linebreak-style": "off",
        "consistent-return": "off",
        "import/no-dynamic-require": "off",
        "no-unused-vars": "off",
        "prefer-const": "off",
        "comma-dangle": "off",
        "no-undef": "off",
        "import/extensions": "off",
        "import/no-unresolved": "off"
    }
}
